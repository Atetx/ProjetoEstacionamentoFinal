@model IEnumerable<EstacionamentoWeb.Models.Estacionar>

@{ ViewData["Title"] = "Retirar"; }

<h1>Estacionar</h1>

<p>
    <a asp-action="Cadastrar" asp-controller="Estacionar" class="botao btn btn-primary">Estacionar</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Veiculo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Veiculo.Placa)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Veiculo)
        <a asp-action="Retirar" asp-route-id="@item.Id" class="botao btn btn-danger">Retirar</a>
    </td>
</tr>
}
    </tbody>
</table>